{
  "name": "cura-wasm-definitions",
  "version": "1.4.0",
  "description": "3D printer definitions for Cura WASM",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "coverage": "nyc --reporter=lcov npm run test",
